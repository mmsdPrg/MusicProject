@model Artist
@{
    ViewData["Title"] = "ArtistPage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid">
    <div class="row row--grid">
        <div class="col-12">
            <div class="article article--page">
                <!-- article content -->
                <div class="article__content">
                    <div class="article__artist">
                        @if (Model.ImagePath != null)
                        {
                            <img src="~/ArtistFile/@(Model.ImagePath)" alt="">

                        }
                        else
                        {
                            <img src="~/Template/img/artists/artist1.jpg" alt="">

                        }
                        <div>
                            <h1>@Model.ArtistName</h1>
                        </div>
                    </div>

                    <p>
                        بیوگرافی :
                        <br />
                        @Model.Biography
                    </p>


                </div>
                <!-- end article content -->
                <!-- share -->
                @* ساخت فیلد شبکه های اجتماعی برای هنرمندان *@
